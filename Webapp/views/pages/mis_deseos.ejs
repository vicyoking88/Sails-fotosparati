<div class="row">
    <div class="col-md-12">
  
      <% flash.get('mensaje').forEach(function (msg) { %>
      <div class="alert alert-info text-center" role="alert">
        <%=msg%>
      </div>
      <% }) %>
  
      <h2>Mis Deseos</h2>
  
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Titulo</th>
            <th scope="col">Foto</th>
          </tr>
        </thead>
        <tbody>
          <% deseos.forEach( deseo => { %>
          <tr>
            <td scope="row"><%=deseo.foto.titulo%></td>
            <td><img src="images/fotos/<%=deseo.foto.contenido%>" alt="" class="wf-10"><a class="btn btn-light mx-2"  href="/delete-deseo/<%=deseo.foto.id%>">Eliminar</a></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>